{
    "version": 2,
    "name": "food-token-generator-application",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "engines": {
        "node": "16.x"
    }
}